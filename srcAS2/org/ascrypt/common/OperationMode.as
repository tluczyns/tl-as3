/**
* Constant values for available operation modes.
* @author Mika Palmu
*/
class org.ascrypt.common.OperationMode
{
	/**
	* Operation mode id for ECB.
	*/
	public static var ECB:String = "ecb";
	
	/**
	* Operation mode id for CBC.
	*/
	public static var CBC:String = "cbc";
	
	/**
	* Operation mode id for CTR.
	*/
	public static var CTR:String = "ctr";
	
	/**
	* Operation mode id for NONE.
	*/
	public static var NONE:String = "none";
	
}
